<script type="text/javascript">
$(function() {
  $('#signin-form').dialog({
  	model: 'User',
    validate: function(data) {
      if (! data.user.email) {
        this.add('email', 'Email is required.');
      }
      if (! data.user.password) {
        this.add('password', 'Password is required.');
      }
    },
  	process: function(data) {
  		// Successful login.
      if ( data.menu_url && data.menu_url.length > 0 )
      {
        location.href = data.menu_url;
      }
      else
      {
        location.reload(true);
      }
  	}
  });

  $(document).on('click.signin', '.show-signin-form', function(ev) {
    ev.preventDefault();
    $('#signin-form').dialog('open');
  });
});
</script>


